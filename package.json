{
  "name": "data-collector",
  "private": true,
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "@activeadmin/activeadmin": "^2.9.0",
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo": "^7.1.0",
    "@hotwired/turbo-rails": "^7.1.0",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "accessible-autocomplete": "^2.0.3",
    "chart.js": "^3.6.0",
    "chartkick": "^4.1.0",
    "esbuild": "^0.14.11",
    "govuk-frontend": "^4.0.1",
    "sass": "^1.47.0",
    "stimulus": "^3.0.0"
  },
  "version": "0.1.0",
  "devDependencies": {},
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --outdir=app/assets/builds",
    "build:css": "sass ./app/assets/stylesheets/application.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules && sass ./app/assets/stylesheets/active_admin.scss ./app/assets/builds/active_admin.css --no-source-map --load-path=node_modules",
    "postinstall": "cp -rT node_modules/govuk-frontend/govuk/assets/fonts app/assets/builds/fonts && cp -rT node_modules/govuk-frontend/govuk/assets/images app/assets/builds/images"
  }
}
